{
    "name": "imarishwa/mpesa-bridge",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Imarishwa\\MpesaBridge\\": "src/"
        },
        "files": [
            "src/Support/helpers.php",
            "src/Support/constants.php"
        ]
    },
    "require": {
        "php": "^7.1.3",
        "guzzlehttp/guzzle": "^6.2",
        "illuminate/support": "~5.6"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Imarishwa\\MpesaBridge\\MpesaBridgeServiceProvider"
            ],
            "aliases": {
                "MpesaBridge": "Imarishwa\\MpesaBridge\\Facades\\MpesaBridge"
            }
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3"
    },
    "autoload-dev": {
        "psr-4": {
            "Imarishwa\\MpesaBridge\\Tests\\": "tests/"
        }
    }
}